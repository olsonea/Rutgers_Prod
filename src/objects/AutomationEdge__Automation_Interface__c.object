        <fullName>AutomationEdge__Total_Execution_Time__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(ISBLANK(AutomationEdge__Response_TimeStamp__c) ,&apos;&apos;,(
TEXT(
ROUND((AutomationEdge__Response_TimeStamp__c - CreatedDate)*24,0)
         )  &amp;&quot; Hours &quot; &amp;
TEXT(
ROUND(MOD((AutomationEdge__Response_TimeStamp__c - CreatedDate)*1440,60),0)
) &amp;&quot; Minutes &quot; &amp;

TEXT(
ROUND(MOD((AutomationEdge__Response_TimeStamp__c - CreatedDate)*86400,60),0)
) &amp;&quot; Seconds &quot;)

)</formula>
        <label>Total Execution Time</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
